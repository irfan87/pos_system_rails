<div class="navbar navbar-static-top navbar-default">
		<div class="container-fluid">
			<div class="navbar-header">
				<%= link_to "Docket System", root_path, class: "navbar-brand" %>
			</div>
			<div class="navbar-collapse collapse navbar-responsive-collapse">
				<ul class="nav navbar-nav">
					<li>
						<%= link_to 'Contact Us', contact_us_path %>
					</li>
				</ul>
				<ul class="nav navbar-nav navbar-right">
					<% if current_user.present? %>
					<li class="dropdown">
						<a href="#" class="dropdown-toggle" data-toggle="dropdown">
							<span class="glyphicon glyphicon-user"></span>
							<%= current_user.username.upcase %>
							<span class="caret"></span>
						</a>
						<ul class="dropdown-menu">
							<li>
								<%= link_to '<span class="glyphicon glyphicon-shopping-cart"></span> Add Shop'.html_safe, "#" %>
							</li>
							<li>
								<%= link_to '<span class="glyphicon glyphicon-list-alt"></span> Profile'.html_safe, "#" %>
							</li>
							<li>
								<%= link_to '<span class="glyphicon glyphicon-envelope"></span> Message'.html_safe, "#" %>
							</li>
							<li class="divider"></li>
							<li>
								<%= link_to '<span class="glyphicon glyphicon-log-out"></span> Log Out'.html_safe, logout_path%>
							</li>
						</ul>
					</li>
					<% else %>
					<li>
						<%= link_to '<span class="glyphicon glyphicon-plus"></span> Sign Up'.html_safe, signup_path %>
					</li>
					<li>
						<%= link_to '<span class="glyphicon glyphicon-log-in"></span> Sign In'.html_safe, signin_path %>
					</li>
					<% end %>
				</ul>
			</div>
		</div>
</div>